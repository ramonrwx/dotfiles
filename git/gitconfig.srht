[sendemail]
  annotate          = yes
  assume8bitEncoing = UTF-8
  chainreplayto     = false
  confirm           = auto
  envelopesender    = auto
  sendmailCmd       = /usr/bin/msmtp
[format]
  signoff = true
  notes   = true
[remote "origin"]
  fetch = +refs/notes/*:refs/notes/*
[notes]
  rewriteRef  = refs/notes/*
  rewriteMode = concatenate
  displayRef  = refs/notes/*
[rerere]
  enabled = true

# vim: set filetype=gitconfig
# vim: ts=2 sts=2 sw=2 et
